{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended",
    "tslint-react"
  ],
  "jsRules": {},
  "rules": {
    "quotemark": [
      true,
      "double",
      "avoid-escape",
      "avoid-template"
    ],
    "return-undefined": true,
    "arrow-parens": false,
    "curly": false,
    "interface-name": false
  },
  "rulesDirectory": []
}
